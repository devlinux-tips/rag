{
  "name": "web-api",
  "version": "1.0.0",
  "description": "Unified Web API for multi-tenant RAG platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:api": "tsx scripts/test-api.ts",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "generate-types": "tsx scripts/generate-types.ts"
  },
  "keywords": [
    "api",
    "rag",
    "multi-tenant",
    "chat",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.8.0",
    "@trpc/client": "^10.45.0",
    "@trpc/server": "^10.45.0",
    "@types/bcrypt": "^6.0.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "date-fns": "^3.0.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^3.3.11",
    "redis": "^4.6.12",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "prisma": "^5.8.0",
    "tsx": "^4.20.5",
    "typescript": "^5.3.3",
    "vitest": "^1.1.1"
  }
}
